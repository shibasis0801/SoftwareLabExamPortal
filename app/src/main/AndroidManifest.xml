<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.overlord.softwarelabexamportal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".common.ChooserActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".common.LoginActivity" />
        <activity
            android:name=".common.CourseActivity"
            android:label="@string/title_activity_course"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".teacher.NewCourseActivity"
            android:label="@string/title_activity_new_course"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".student.GiveExamActivity"
            android:label="@string/title_activity_enroll_course"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>